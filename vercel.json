{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/backend/:path*",
      "destination": "/backend/:path*"
    },
    {
      "source": "/frontend/:path*",
      "destination": "/frontend/:path*"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/frontend/:path*"
    }
  ]
}